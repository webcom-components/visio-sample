{
  "name": "visio-webcom-sample",
  "version": "1.0.0",
  "description": "visio web application with Webcom",
  "scripts": {
    "start": "webpack-dev-server --hot --host 0.0.0.0",
    "build": "NODE_ENV=production webpack --progress --colors -p",
    "serve": "NODE_ENV=production webpack-dev-server --host 0.0.0.0",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --compilers js:babel-core/register",
    "semantic-release": "semantic-release pre  && semantic-release post"
  },
  "keywords": [
    "webcom",
    "webrtc"
  ],
  "author": "Paul Musso<paul.musso@orange.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.4.0",
    "babel-plugin-transform-react-inline-elements": "^6.4.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.1.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^1.10.1",
    "expose-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "ghooks": "^1.0.3",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.6.5",
    "isparta": "^4.0.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "mocha": "^2.3.4",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "semantic-release": "^4.3.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "validate-commit-msg": "^1.1.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0-rc4",
    "redux": "^3.0.4",
    "redux-devtools": "^3.0.3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-form": "^4.1.4",
    "redux-logger": "^2.0.4",
    "redux-promise": "^0.5.0",
    "redux-simple-router": "^2.0.3",
    "redux-thunk": "^1.0.0",
    "webcom": "^0.2.2",
    "webcom-reach": "^1.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webcom-components/visio-sample.git"
  }
}
